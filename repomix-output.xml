            'n_layers': 4,
            'd_ff': 1024,
            'dropout': 0.1,
            'output_dim': 256
        },
        'training_params': {
            'n_epochs': 100,
            'batch_size': 32,
            'learning_rate': 1e-4
        }
    }
    
    device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')
    print(f"Using device: {device}")
    
    try:
        # Create dataset
        print("\nüìä Creating genomic dataset...")
        dataset, metadata = create_genomic_dataset(config['enhanced_data_path'])
        print(f"‚úÖ Dataset created: {len(dataset)} patients, {metadata['n_genetic_features']} genetic features")
        
        # Print genetic feature statistics
        print(f"\nüß¨ Genetic feature statistics:")
        for feature, stats in metadata['feature_statistics'].items():
            print(f"  {feature}:")
