    print(f"üß¨ Genomic embeddings: {integration.genomic_embeddings.shape}")
    print(f"üéØ Prognostic targets: {integration.prognostic_targets.shape}")
    print(f"üìà Motor progression R¬≤: {evaluation_results['motor_r2']:.4f}")
