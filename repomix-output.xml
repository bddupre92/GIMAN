        print(f"   - {name}: {analysis['null_percentage']:.1f}% null")

    print(f"\nüìã Missing EVENT_ID datasets: {len(missing_datasets)}")
    for name in missing_datasets:
        print(f"   - {name}")

    # Create standardization mapping
    print("\nüîß PROPOSED STANDARDIZATION:")
