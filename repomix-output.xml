    "        static_data[dataset_name] = df\n",
    "        print(f\"   üìä STATIC (no EVENT_ID)       {dataset_name:<40} | Patients: {patients:4d}\")\n",
    "\n",
    "print(f\"\\nüîÑ CREATING PROPER MERGED DATASETS:\")\n",
    "\n",
    "print(f\"\\nüìä STATIC BASELINE REGISTRY (PATNO-only merge):\")\n",
