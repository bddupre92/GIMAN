        print(f"   üìÑ Metadata: {metadata_path}")
        print(f"   üîó Latest: {latest_dataset_path}")
        
        return str(dataset_path)
        
    def create_feature_comparison_report(self, enhanced_df: pd.DataFrame, metadata: Dict):
